{
    "manifest_version": 3,
    "name": "YouTube Extension",
    "version": "1.0",
    "description": "A YouTube extension that extracts video URLs from channels",
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "js": ["contentScript.js"],
        "matches": ["https://www.youtube.com/@MarkRober/community"]
      }
    ],
    "permissions": ["activeTab", "scripting", "tabs"]
  }